<!DOCTYPE>
<html lang="ko">
<head>
    <title>에디터 테스트</title>
    <!-- Bootstrap core CSS     -->
    <link href="/css/bootstrap.css" rel="stylesheet" />
    <!--   Core JS Files   -->
    <script src="/js/jquery-1.11.3.min.js" type="text/javascript"></script>
    <script src="/js/bootstrap.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="/ckeditor/ckeditor.js"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            // 초기 설정.
            CKEDITOR.replace("editText", {
                toolbar:[],
                "width" : "800",
                "height" : "50"
            });
            CKEDITOR.instances.editText.setData("");
            // 버튼 전송처리.
            $("#submitBtn").on("click", function() {
                // 폼처리.
                $("#frm").attr("method", "post");
                $("#frm").attr("action", "/test/editor3/process");
                $("#frm").submit();
            });
        });
    </script>
</head>
<body>
<form id="frm" name="frm">
<div id="nav">
    <h1>테스터 에디터</h1>
</div>
<div id="content">
    <textarea id="editText" name="editText" wrap="hard" style="width:50%; height:10%;"></textarea>
</div>
<div>
    <input type="button" id="submitBtn" name="submitBtn" value="전송"/>
</div>
<div id="footer`">
    푸터
</div>
</form>
</body>
</html>
<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>회원가입</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="/css/join.css">
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <!-- Magnific Popup -->
    <script type="text/javascript" src="/js/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="/js/magnific-popup-options.js"></script>
    <script type="text/javascript" src="https://spi.maps.daum.net/imap/map_js_init/postcode.v2.js"></script>
    <script type="text/javascript">
        var emailRegExp = /^[0-9a-zA-Z]([-_\.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_\.]?[0-9a-zA-Z])*\.[a-zA-Z]{2,3}$/i;
        var cellNoRegExp = /^\d{3}-\d{3,4}-\d{4}$/;
        var telNoRegExp = /^\d{2,3}-\d{3,4}-\d{4}$/;
        var idRegExp = /^[a-z0-9_]{4,20}$/;
        var pwdRegExp = /^(?=.*[a-z])(?=.*[0-9]).{6,16}$/;
        $(document).ready(function() {
            // init 초기화 처리.
            var idCheck = false;
            var emailCheck = false;

            $("#companySpan").css("display","none");
            $("#srchPostNoBtn1").bind("click",function() {
                execDaumPostcode('1');
            });
            $("#srchPostNoBtn2").bind("click",function() {
                execDaumPostcode('2');
            });

            // 회원유형 버튼 이벤트 처리시.
            $("#usrType1").on("click", function() {
                $("#companySpan").css("display","none");
            });
            $("#usrType2").on("click", function() {
                $("#companySpan").css("display","none");
            });
            $("#usrType3").on("click", function() {
                $("#companySpan").css("display","block");
            });
            $("#usrType4").on("click", function() {
                $("#companySpan").css("display","block");
            });
            $("#usrType5").on("click", function() {
                $("#companySpan").css("display","block");
            });
            // 이메일 '직접입력' 선택시 처리.
            $("#usrEmail2").on("change", function() {
                if($("#usrEmail2 option:selected").val()=="direct") {
                    $("#usrEmail3").attr("readonly",false);
                } else {
                    $("#usrEmail3").val("");
                    $("#usrEmail3").attr("readonly",true);
                }
            });
            // 주소카피 버튼 이벤트 처리
            $("#copyBtn").on("click", function() {
                if($("#copyBtn").prop("checked")) {
                    $("#usrCompPostNo").val($("#usrPostNo").val());
                    $("#usrCompAddr1").val($("#usrAddr1").val());
                    $("#usrCompAddr2").val($("#usrAddr2").val());
                } else {
                    $("#usrCompPostNo").val("");
                    $("#usrCompAddr1").val("");
                    $("#usrCompAddr2").val("");
                }
            });
            // 아이디 중복 체크 버튼 이벤트 처리.
            $("#idChkBtn").on("click",function() {

                if($("#usrId").val() == null || $("#usrId").val() == "") {
                    alert("아이디를 입력하세요.");
                    $("#usrId").focus();
                    return false;
                }

                if(!idRegExp.test($("#usrId").val())) {
                    alert("올바른 아이디가 아닙니다.\n 다시 확인하시고 입력하세요.");
                    $("#usrId").focus();
                    return false;
                }

                // 값 저장 처리.
                var dataVals = {
                    usrId : $.trim($("#usrId").val()),
                };
                // 전송 처리.
                $.ajax({
                    url: "/common/idCheckProcess",
                    type: "post",
                    dataType: "json",
                    data: dataVals,
                    success: function (data) {
                        if(data.result > 0) {
                            idCheck = false;
                            alert("이미 사용중인 아이디가 있습니다.");
                            $("#usrId").focus();
                        } else {
                            idCheck = true;
                            alert("사용가능한 아이디 입니다.");
                            $("#usrId").attr("readonly",true);
                            $("#usrPwd1").focus();
                        }
                    },
                    error: function (err) {
                        alert("errCode : " + err);
                    }
                });
            });

            // 이메일 중복 체크 버튼 이벤트 처리.
            $("#emailChkBtn").on("click",function() {
                var usrEmail;
                // 이메일2 값 취득.
                var usrEmail1 = $.trim($("#usrEmail1").val());
                var usrEmail2 = $.trim($("#usrEmail2 option:selected").val());
                var usrEmail3 = $.trim($("#usrEmail3").val());

                if(usrEmail1 == "") {
                    alert("이메일 아이디를 입력하여 주세요.");
                    $("#usrEmail1").focus();
                    return false;
                }
                if(usrEmail2 == "") {
                    alert("이메일 주소를 선택하여 주세요.");
                    $("#usrEmail2").focus();
                    return false;
                } else {
                    if(usrEmail2 == "direct") {
                        if($("#usrEmail3").val()==null || $("#usrEmail3").val()=="") {
                            alert("이메일 주소를 입력하여 주세요.");
                            $("#usrEmail3").focus();
                            return false;
                        }
                    }

                }
                // 값셋팅.
                if(usrEmail1 != "" && usrEmail2 != "direct") {
                    usrEmail = usrEmail1 + "@" + usrEmail2;
                } else if(usrEmail1 != "" && usrEmail2 == "direct") {
                    if($("#usrEmail3").val() != null || $("#usrEmail3").val() != "") {
                        usrEmail = usrEmail1 + "@" + usrEmail3;
                    }
                }
                // 이메일 상세 체크.
                if(!emailRegExp.test(usrEmail)) {
                    alert("올바른 이메일이 아닙니다. 다시 입력하여 주세요.");
                    $("#usrEmail1").focus();
                    return false;
                }

                // 값 저장 처리.
                var dataVals = {
                    usrEmail : usrEmail
                };
                // 전송 처리.
                $.ajax({
                    url: "/common/emailCheckProcess",
                    type: "post",
                    dataType: "json",
                    data: dataVals,
                    success: function (data) {
                        console.log(">>> data = " + data.result);
                        if(data.result > 0) {
                            alert("이미 사용중인 이메일이 있습니다.");
                            $("#usrEmail1").focus();
                        } else {
                            emailCheck = true;
                            alert("사용가능한 이메일 입니다.");
                            $("#usrEmail1").attr("readonly",true);
                            $("#usrEmail2").attr("disabled",true);
                            $("#usrEmail3").attr("readonly",true);
                        }
                    },
                    error: function (err) {
                        alert("errCode : " + err);
                    }
                });
            });

            // a 링크 클릭이벤트 처리.
            $("#aSubmitBtn").on("click", function(e) {
                e.preventDefault();
                // 초기화.
                var usrCompNm = "";
                var usrCompTelNo = "";
                var usrCompAddr1 = "";
                var usrCompAddr2 = "";
                var usrCompPostNo = "";
                var usrSaupNo = "";
                // 회원유형 값 취득.
                var userType = $("input:radio[name='usrType']:checked").val();
                // 생일월별 값 취득.
                var usrBirthYear = $("#usrBirthYear option:selected").val();
                var usrBirthMonth = $("#usrBirthMonth option:selected").val();
                var usrBirthDay = $("#usrBirthDay option:selected").val();
                // 성별 값 취득.
                var usrSex = $("input:radio[name='usrSex']:checked").val();
                // 연락처 앞번호 취득.
                var usrTelNo1 = $("#usrTelNo1 option:selected").val();
                // 휴대폰 앞번호 취득.
                var usrCellNo1 = $("#usrCellNo1 option:selected").val();
                // 이메일 뒷 자리 취득.
                var usrEmail2 = $("#usrEmail2 option:selected").val();
                // 회사 전화번호 취득.
                var usrCompTelNo1 = $("#usrCompTelNo1 option:selected").val();
                // 정보활용확인여부 취득.
                var usrInfoYn = $("input:radio[name='usrInfoYn']:checked").val();

                // 고객명 체크.
                if($("#usrName").val() == null || $("#usrName").val() == "") {
                    alert("성함을 입력하여 주세요.");
                    $("#usrName").focus();
                    return false;
                }
                // 고객 아이디 체크
                if($("#usrId").val() == null || $("#usrId").val() == "") {
                    alert("고객 아이디를 입력하여 주세요.");
                    $("#usrId").focus();
                    return false;
                }
                // 고객 아이디 체크
                if($("#usrId").val() == null || $("#usrId").val() == "") {
                    alert("고객 아이디를 입력하여 주세요.");
                    $("#usrId").focus();
                    return false;
                }
                if(idCheck==false) {
                    alert("아이디 중복 확인 해주세요.");
                    $("#").focus();
                    return false;
                }

                // 고객 비밀번호1 체크
                if($("#usrPwd1").val() == null || $("#usrPwd1").val() == "") {
                    alert("고객 비밀번호를 입력하여 주세요.");
                    $("#usrPwd1").focus();
                    return false;
                }
                // 고객 비밀번호2 체크
                if($("#usrPwd2").val() == null || $("#usrPwd2").val() == "") {
                    alert("확인용 고객 비밀번호를 입력하여 주세요.");
                    $("#usrPwd2").focus();
                    return false;
                }
                // 입력한 고객비밀번호가 맞는 지 체크.
                if($("#usrPwd1").val() != $("#usrPwd2").val()) {
                    alert("입력하신 비밀번호가 맞지 않습니다.\n 다시 확인해주세요.");
                    $("#usrPwd2").focus();
                    return false;
                } else {
                    // 비밀번호 정규화 체크.
                    if(!pwdRegExp.test($("#usrPwd2").val())) {
                        alert("비밀번호 형식이 올바르지 않습니다.\n 영문 소문자/숫자를 혼용하여 입력하여주세요.\n (최소6자리)");
                        $("#usrPwd2").val("");
                        $("#usrPwd1").focus();
                        return false;
                    }
                }
                // 생년월일 체크.
                if($.trim(usrBirthYear) == "") {
                    alert("생년을 선택하여주세요.");
                    $("#usrBirthYear").focus();
                    return false;
                }
                if($.trim(usrBirthMonth) == "") {
                    alert("월을 선택하여주세요.");
                    $("#usrBirthYear").focus();
                    return false;
                }
                if($.trim(usrBirthDay) == "") {
                    alert("일을 선택하여주세요.");
                    $("#usrBirthYear").focus();
                    return false;
                }

                // 우편번호 체크
                if($("#usrPostNo").val() == null || $("#usrPostNo").val() == "") {
                    alert("우편번호 검색하여 입력하여 주세요.");
                    $("#srchPostNoBtn1").focus();
                    return false;
                }
                // 주소 체크
                if($("#usrAddr1").val() == null || $("#usrAddr1").val() == "") {
                    alert("우편번호(2) 검색하여 입력하여 주세요.");
                    $("#srchPostNoBtn1").focus();
                    return false;
                }

                // 연락처 체크
                if(usrTelNo1 == null || usrTelNo1 == "") {
                    alert("전화번호 앞자리를 선택하여 주세요.");
                    $("#usrTelNo1").focus();
                    return false;
                }
                if($("#usrTelNo2").val() == null || $("#usrTelNo2").val() == "") {
                    alert("전화번호 두번째 자리를 입력하여 주세요.");
                    $("#usrTelNo2").focus();
                    return false;
                }
                if($("#usrTelNo3").val() == null || $("#usrTelNo3").val() == "") {
                    alert("전화번호 세번째 자리를 입력하여 주세요.");
                    $("#usrTelNo3").focus();
                    return false;
                }
                // 휴대폰 체크.
                if(usrCellNo1 == null || usrCellNo1 == "") {
                    alert("휴대폰 번호 앞자리를 선택하여 주세요.");
                    $("#usrCellNo1").focus();
                    return false;
                }
                if($("#usrCellNo2").val() == null || $("#usrCellNo2").val() == "") {
                    alert("휴대폰 번호 두번째 자리를 입력하여 주세요.");
                    $("#usrCellNo2").focus();
                    return false;
                }
                if($("#usrCellNo3").val() == null || $("#usrCellNo3").val() == "") {
                    alert("휴대폰 번호 세번째 자리를 입력하여 주세요.");
                    $("#usrCellNo3").focus();
                    return false;
                }
                // 이메일 체크.
                if($("#usrEmail1").val() == null || $("#usrEmail1").val() == "") {
                    alert("이메일 앞자리를 입력하여 주세요.");
                    $("#usrEmail1").focus();
                    return false;
                }
                if(usrEmail2 == null || usrEmail2 == "") {
                    alert("이메일 주소를 선택하여 주세요.");
                    $("#usrEmail2").focus();
                    return false;
                } else {
                    if(usrEmail2 == "direct") {
                        if($("#usrEmail3").val() == null || $("#usrEmail3").val() == "") {
                            alert("이메일 주소를 입력하여 주세요.");
                            $("#usrEmail3").focus();
                            return false;
                        }
                    }
                }
                if(emailCheck==false) {
                    alert("이메일 중복 확인 해주세요.");
                    $("#usrEmail1").focus();
                    return false;
                }
                // 사업자인 경우 체크.
                if(userType=="C" || userType=="D" || userType=="E") {
                    // 회사명 체크.
                    if($("#usrCompNm").val() == null || $("#usrCompNm").val() == "") {
                        alert("회사명을 입력하여 주세요.");
                        $("#usrCompNm").focus();
                        return false;
                    }
                    // 회사전화 체크.
                    if(usrCompTelNo1 == null || usrCompTelNo1 == "") {
                        alert("전화번호 앞자리를 선택하여 주세요.");
                        $("#usrCompTelNo1").focus();
                        return false;
                    }
                    if($("#usrCompTelNo2").val() == null || $("#usrCompTelNo2").val() == "") {
                        alert("전화번호 두번째 자리를 입력하여 주세요.");
                        $("#usrCompTelNo2").focus();
                        return false;
                    }
                    if($("#usrCompTelNo3").val() == null || $("#usrCompTelNo3").val() == "") {
                        alert("전화번호 세번째 자리를 입력하여 주세요.");
                        $("#usrCompTelNo3").focus();
                        return false;
                    }
                    usrCompTelNo = usrCompTelNo1 + "-" + $("#usrCompTelNo2").val() + "-" + $("#usrCompTelNo3").val();
                    if(!telNoRegExp.test(usrCompTelNo)) {
                        alert("회사 전화번호 형식이 올바르지 않습니다.\n 확인하고 다시 입력하여 주세요.");
                        $("#usrCompTelNo2").focus();
                        return false;
                    }
                    // 회사우편번호 체크
                    if($("#usrCompPostNo").val() == "" || $("#usrCompPostNo").val() == "") {
                        alert("우편번호 검색하여 입력하여 주세요.");
                        $("#srchPostNoBtn2").focus();
                        return false;
                    }
                    // 회사주소 체크.
                    if($("#usrCompAddr1").val() == null || $("#usrCompAddr1").val() == "") {
                        alert("우편번호 검색하여 입력하여 주세요.");
                        $("#srchPostNoBtn2").focus();
                        return false;
                    }
                    // 사업자등록번호 체크.
                    if($("#usrSaupNo").val() == null || $("#usrSaupNo").val() == "") {
                        alert("사업자번호를 입력하여 주세요.");
                        $("#usrSaupNo").focus();
                        return false;
                    } else {
                        if($("#usrSaupNo").val().length != 12) {
                            alert("사업자등록번호가 정확하지 않습니다. \n '-' 까지 포함하여(12자리) 입력하여 주세요.");
                            $("#usrSaupNo").focus();
                            return false;
                        }
                    }
                    // 값 취득.
                    usrCompNm = $("#usrCompNm").val();
                    usrCompPostNo = $("#usrCompPostNo").val();
                    usrCompAddr1 = $("#usrCompAddr1").val();
                    usrCompAddr2 = $("#usrCompAddr2").val();
                    usrSaupNo = $("#usrSaupNo").val();
                }
                // 정보활용확인 여부 체크.
                if(usrInfoYn=="N") {
                    alert("정보활용확인여부 '예'로 체크하셔야 가입이 가능합니다.");
                    $("#usrInfoYn1").focus();
                    return false;
                }

                // val
                var usrBirth = usrBirthYear + "" + usrBirthMonth + "" + usrBirthDay;
                var usrPostNo = $("#usrPostNo").val();
                var usrTelNo = usrTelNo1 + "-" + $("#usrTelNo2").val() + "-" + $("#usrTelNo3").val();
                var usrCellNo = usrCellNo1 + "-" + $("#usrCellNo2").val() + "-" + $("#usrCellNo3").val();

                // 전화번호/휴대폰/회사번호 정규화 체크.
                if(!telNoRegExp.test(usrTelNo)) {
                    alert("전화번호 형식이 올바르지 않습니다.\n 확인하고 다시 입력하여 주세요.");
                    $("#usrTelNo2").focus();
                    return false;
                }
                if(!cellNoRegExp.test(usrCellNo)) {
                    alert("휴대폰 번호 형식이 올바르지 않습니다.\n 확인하고 다시 입력하여 주세요.");
                    $("#usrCellNo2").focus();
                    return false;
                }


                // 값셋팅.
                if($.trim($("#usrEmail1").val()) != "" && usrEmail2 != "direct") {
                    var usrEmail = $("#usrEmail1").val() + "@" + usrEmail2;
                } if($.trim($("#usrEmail1").val()) != "" && usrEmail2 == "direct") {
                    if($("#usrEmail3").val() != null || $.trim($("#usrEmail3").val()) != "") {
                        var usrEmail = $("#usrEmail1").val() + "@" + $("#usrEmail3").val();
                    }
                }

                // 값 저장 처리.
                var dataVals = {
                    usrType : userType,
                    usrId : $.trim($("#usrId").val()),
                    usrPwd : $.trim($("#usrPwd1").val()),
                    usrName : $.trim($("#usrName").val()),
                    usrAddr1 : $.trim($("#usrAddr1").val()),
                    usrAddr2 : $.trim($("#usrAddr2").val()),
                    usrPostNo : usrPostNo,
                    usrEmail : usrEmail,
                    usrSex : usrSex,
                    usrBirth : usrBirth,
                    usrJuminNo : '',
                    usrTelNo : usrTelNo,
                    usrTelNo1 : usrTelNo1,
                    usrTelNo2 : $.trim($("#usrTelNo2").val()),
                    usrTelNo3 : $.trim($("#usrTelNo3").val()),
                    usrCellNo : usrCellNo,
                    usrCellNo1 : usrCellNo1,
                    usrCellNo2 : $.trim($("#usrCellNo2").val()),
                    usrCellNo3 : $.trim($("#usrCellNo3").val()),
                    usrCompNm : usrCompNm,
                    usrCompTelNo : usrCompTelNo,
                    usrCompTelNo1 : usrCompTelNo1,
                    usrCompTelNo2 : $.trim($("#usrCompTelNo2").val()),
                    usrCompTelNo3 : $.trim($("#usrCompTelNo3").val()),
                    usrCompAddr1 : usrCompAddr1,
                    usrCompAddr2 : usrCompAddr2,
                    usrCompPostNo : usrCompPostNo,
                    usrSaupNo : usrSaupNo,
                    usrEmailYn : "Y",
                    usrInfoYn : usrInfoYn
                };
                // 전송 처리.
                $.ajax({
                    url: "/signup/app/signupProcess",
                    type: "post",
                    dataType: "json",
                    data: dataVals,
                    success: function (data) {
                        console.log(">>> data = " + data.result);
                        if(data.result=="OK") {
                            alert("가입처리 되었습니다. 감사합니다.");
                            $("#aSubmitBtn").attr("disabled", "true");
                            /* 홈페이지 완성되면 풀것!
                             if(window.parent == window.top) {
                             window.parent.$.magnificPopup.close();
                             }
                             location.href = "/";
                             */
                        }
                    },
                    error: function (err) {
                        alert("errCode : " + err.message);
                    }
                });

            });
        });
        function execDaumPostcode(no) {
            new daum.Postcode({
                oncomplete: function(data) {
                    // 팝업에서 검색결과 항목을 클릭했을때 실행할 코드를 작성하는 부분.

                    // 각 주소의 노출 규칙에 따라 주소를 조합한다.
                    // 내려오는 변수가 값이 없는 경우엔 공백('')값을 가지므로, 이를 참고하여 분기 한다.
                    var fullAddr = ''; // 최종 주소 변수
                    var extraAddr = ''; // 조합형 주소 변수

                    // 사용자가 선택한 주소 타입에 따라 해당 주소 값을 가져온다.
                    if (data.userSelectedType === 'R') { // 사용자가 도로명 주소를 선택했을 경우
                        fullAddr = data.roadAddress;

                    } else { // 사용자가 지번 주소를 선택했을 경우(J)
                        fullAddr = data.jibunAddress;
                    }

                    // 사용자가 선택한 주소가 도로명 타입일때 조합한다.
                    if(data.userSelectedType === 'R'){
                        //법정동명이 있을 경우 추가한다.
                        if(data.bname !== ''){
                            extraAddr += data.bname;
                        }
                        // 건물명이 있을 경우 추가한다.
                        if(data.buildingName !== ''){
                            extraAddr += (extraAddr !== '' ? ', ' + data.buildingName : data.buildingName);
                        }
                        // 조합형주소의 유무에 따라 양쪽에 괄호를 추가하여 최종 주소를 만든다.
                        fullAddr += (extraAddr !== '' ? ' ('+ extraAddr +')' : '');
                    }

                    if(no=="1") {
                        // 우편번호와 주소 정보를 해당 필드에 넣는다.
                        document.getElementById('usrPostNo').value = data.zonecode; //5자리 새우편번호 사용
                        document.getElementById('usrAddr1').value = fullAddr;
                        // 커서를 상세주소 필드로 이동한다.
                        document.getElementById('usrAddr2').focus();
                    }
                    if(no=="2") {
                        // 우편번호와 주소 정보를 해당 필드에 넣는다.
                        document.getElementById('usrCompPostNo').value = data.zonecode; //5자리 새우편번호 사용
                        document.getElementById('usrCompAddr1').value = fullAddr;
                        // 커서를 상세주소 필드로 이동한다.
                        document.getElementById('usrCompAddr2').focus();
                    }

                }
            }).open();
        }

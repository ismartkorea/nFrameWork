<% layout('../../admin/layout') -%>
<script type="text/javascript" src="/js/jquery-1.11.3.min.js"></script>
<script type="text/javascript">
    $(document).ready(function() {

        // 이전 화면 이동 버튼 이벤트 처리.
        $("#prevBtn").on('click', function(e) {
            e.preventDefault();
            location.href = '/admin/coupon';
        });
        // 삭제 버튼 이벤트 처리.
        $("#editBtn").on('click', function(e) {
            e.preventDefault();
            location.href = "/admin/coupon/edit/<%=result.couponNo%>";
        });
        // 삭제 버튼 이벤트 처리.
        $("#delBtn").on('click', function(e) {
            e.preventDefault();
            location.href = "/admin/coupon/delete/<%=result.couponNo%>";
        });
    });
</script>
<div class="container-fluid">

    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">버그 리포트 화면</h1>
        </div>
        <form name="frm" id="frm" method="post" class="form-horizontal">
            <input type="hidden" id="no" name="no" value="<%=result.couponNo%>"/>
            <table class="table table-bordered">
                <tr>
                    <td style="width:80px;">
                        <label for="couponNo">쿠폰번호</label>
                    </td>
                    <td>
                        <%= result.couponNo %>
                    </td>
                </tr>
                <tr>
                    <td style="width:80px;">
                        <label for="couponStatus">쿠폰 상태</label>
                    </td>
                    <td>
                        <%=result.couponStatus %>
                    </td>
                </tr>
                <tr>
                    <td>
                        <label for="insDate">작성날짜</label>
                    </td>
                    <td>
                        <%=result.insDate%>
                    </td>
                </tr>
                <tr>
                    <td style="width:80px;">
                        <label for="insUsrNm">작성자</label>
                    </td>
                    <td>
                        <%=result.insUsrNm%>
                    </td>
                </tr>
            </table>
            <p class="form-group">
                <div class="col-sm-offset-2 col-sm-10 text-right">
                    <input type="button" id="prevBtn" name="prevBtn" class="btn btn-default" value="리스트 화면"/>&nbsp;
                    <input type="button" id="editBtn" name="editBtn" class="btn btn-default" value="수정"/>
                    <input type="button" id="delBtn" name="delBtn" class="btn btn-default" value="삭제"/>
                </div>
            </p>
        </form>
    </div>
        </form>
    </div>
</div>